{
  "expo": {
    "name": "AdpgApp",
    "slug": "adpg",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/adpg-app-icon.png",
    "scheme": "adpgapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brunopimentelff.EDPG",
      "buildNumber": "30",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adpg-app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.brunopimentelff.EDPG",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "582299c8-5d03-42b7-8cf3-e5f8010d7bed"
      }
    },
    "owner": "pedroteles"
  }
}
